{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "ignorePaths": [],
  "baseBranches": [
    "master",
    "renovate-testcontainers"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "src/test/java/.+\\.java$"
      ],
      "matchStrings": [
        "DockerImageName\\.parse\\(\"(?<depName>.*?):(?<currentValue>.*?)\"\\)"
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
